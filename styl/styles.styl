*
	margin 0
	padding 0
	outline none
body
	font-family: 'RobotoLight', sans-serif
	font-size: 22px
	background: black
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration: none
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
h1, h2, h3,  h4, h5, h6
	font-family: 'OswaldBold', sans-serif;
.button
	display block
	width: 261px;
	height: 60px;
	line-height: 60px;
	text-align: center;
	font-size: 18px;
	font-family: "RobotoBold", sans-serif;
	color: rgb(13, 32, 56);
	background-color: c_green
	border-radius: 30px;
	transition: .4s
	&:focus
		color: rgb(13, 32, 56);
		text-decoration: none
	&:hover
		color rgb(13, 32, 56);
		text-decoration: none 
		background-color: #01ffc6
	// transform-style: preserve-3d;
	// perspective: 1000px;
	// transform-origin: center center;
	position: relative
	margin: 0 auto
